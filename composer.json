{
    "name": "urlcv/timezone-overlap",
    "description": "DST-aware working-hours overlap visualiser for two cities. Powers the free Time Zone Overlap tool at urlcv.com.",
    "type": "library",
    "license": "MIT",
    "keywords": ["timezone", "overlap", "working-hours", "dst", "recruitment", "remote-work"],
    "homepage": "https://urlcv.com/tools/timezone-overlap",
    "authors": [
        {
            "name": "URLCV",
            "homepage": "https://urlcv.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "URLCV\\TimezoneOverlap\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "URLCV\\TimezoneOverlap\\Laravel\\TimezoneOverlapServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
